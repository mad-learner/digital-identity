<div>
    <h2> Test Application </h2>
    <h4> Scan QR Code to log in</h4>

    <div id="qrcode"></div>
    <script type="text/javascript">
        var data = {
            publicKey: "abc123",
            name: "Test Application",
            permissions: [
                "name",
                "city"
            ]
        }
        var qrcode = new QRCode("test", {
            text: JSON.stringify(data),
            width: 128,
            height: 128,
            colorDark : "#000000",
            colorLight : "#ffffff",
            correctLevel : QRCode.CorrectLevel.H
        });
    </script>
</div>